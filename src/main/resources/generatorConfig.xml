<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <context id="testTables" targetRuntime="MyBatis3">
        <commentGenerator>
            <!-- 是否去除自动生成的注释 true：是 ： false:否 一般选是 -->
            <property name="suppressAllComments" value="true" />
        </commentGenerator>
        <!--数据库连接的信息：驱动类、连接地址、用户名、密码 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/cskaoyanmall"
                        userId="root"
                        password="080514">
            <!--是否去除同名表 true表示仅仅在一个数据库中找表，不会找到其他数据库的同名表-->
            <property name="nullCatalogMeansCurrent" value="true"/>
        </jdbcConnection>
        <!-- javaModelGenerator javaBean生成的配置信息
             targetProject:生成PO类的位置
             targetPackage：生成PO类的类名-->

        <!--javabean的目标地址-->
        <javaModelGenerator targetPackage="com.cskaoyan.bean"
                            targetProject=".\src\main\java">
            <!-- enableSubPackages:是否允许子包,是否让schema作为包的后缀
                 即targetPackage.schemaName.tableName -->
            <property name="enableSubPackages" value="true" />
            <!-- 从数据库返回的值是否清理前后的空格 -->
            <property name="trimStrings" value="true" />
        </javaModelGenerator>


        <!-- sqlMapGenerator Mapper映射文件的配置信息
            targetProject:mapper映射文件生成的位置
            targetPackage:生成mapper映射文件放在哪个包下-->
        <!--映射文件的目标地址-->
        <sqlMapGenerator targetPackage="com.cskaoyan.mapper"
                         targetProject=".\src\main\resources">
            <!-- enableSubPackages:是否让schema作为包的后缀 -->
            <property name="enableSubPackages" value="true" />
        </sqlMapGenerator>

        <!--
           javaClientGenerator 生成 Model对象(JavaBean)和 mapper XML配置文件 对应的Dao代码
           targetProject:mapper接口生成的位置
           targetPackage:生成mapper接口放在哪个包下

           ANNOTATEDMAPPER
           XMLMAPPER
           MIXEDMAPPER
        -->
        <!--mapper接口的目标地址-->
        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="com.cskaoyan.mapper"
                             targetProject=".\src\main\java"><!--/-->
            <!-- enableSubPackages:是否让schema作为包的后缀 -->
            <property name="enableSubPackages" value="true" />
        </javaClientGenerator><!---->
        <!-- 指定数据库表 -->

            <!-- 指定所有数据库表 -->

            <!--<table tableName="%"
                   enableCountByExample="false"
                   enableUpdateByExample="false"
                   enableDeleteByExample="false"
                   enableSelectByExample="false"
                   enableInsert="false"
                   enableDeleteByPrimaryKey="true"
                   enableSelectByPrimaryKey="true"
                   selectByExampleQueryId="false" ></table>-->

               <!-- 指定数据库表，要生成哪些表，就写哪些表，要和数据库中对应，不能写错！ -->
                <!--byExample 构造where条件 和你的列相关 几乎涵盖了所有你能想到的条件 -->
                <!--User User UserMapper UserMapper.xml-->
                <!--domainObjectName xxx  则bean:xxx 接口：xxxMapper 映射文件：xxxMapper.xml-->
                <table tableName="cskaoyanmall_ad" domainObjectName="Ad"/>
                <table tableName="cskaoyanmall_address" domainObjectName="Address"/>
                <table tableName="cskaoyanmall_admin" domainObjectName="Admin"/>
                <table tableName="cskaoyanmall_brand" domainObjectName="Brand"/>
                <table tableName="cskaoyanmall_cart" domainObjectName="Cart"/>
                <table tableName="cskaoyanmall_category" domainObjectName="Category"/>
                <table tableName="cskaoyanmall_collect" domainObjectName="Collect"/>
                <table tableName="cskaoyanmall_comment" domainObjectName="Comment"/>
                <table tableName="cskaoyanmall_coupon" domainObjectName="Coupon"/>
                <table tableName="cskaoyanmall_coupon_user" domainObjectName="CouponUser"/>

                <table tableName="cskaoyanmall_feedback" domainObjectName="Feedback"/>
                <table tableName="cskaoyanmall_footprint" domainObjectName="Footprint"/>
                <table tableName="cskaoyanmall_goods" domainObjectName="Goods"/>
                <table tableName="cskaoyanmall_goods_attribute" domainObjectName="GoodsAttribute"/>
                <table tableName="cskaoyanmall_goods_product" domainObjectName="GoodsProduct"/>
                <table tableName="cskaoyanmall_goods_specification" domainObjectName="GoodsSpecification"/>
                <table tableName="cskaoyanmall_groupon" domainObjectName="Groupon"/>
                <table tableName="cskaoyanmall_groupon_rules" domainObjectName="GrouponRules"/>
                <table tableName="cskaoyanmall_issue" domainObjectName="Issue"/>
                <table tableName="cskaoyanmall_keyword" domainObjectName="Keyword"/>

                <table tableName="cskaoyanmall_log" domainObjectName="Log"/>
                <table tableName="cskaoyanmall_order" domainObjectName="Order"/>
                <table tableName="cskaoyanmall_order_goods" domainObjectName="OrderGoods"/>
                <table tableName="cskaoyanmall_permission" domainObjectName="Permission"/>
                <table tableName="cskaoyanmall_region" domainObjectName="Region"/>
                <table tableName="cskaoyanmall_role" domainObjectName="Role"/>
                <table tableName="cskaoyanmall_search_history" domainObjectName="SearchHistory"/>
                <table tableName="cskaoyanmall_storage" domainObjectName="Storage"/>
                <table tableName="cskaoyanmall_system" domainObjectName="System"/>
                <table tableName="cskaoyanmall_topic" domainObjectName="Topic"/>

                <table tableName="cskaoyanmall_user" domainObjectName="User"/>
                <table tableName="cskaoyanmall_user_formid" domainObjectName="UserFormid"/>







        <!--      <table schema="" tableName="orders"></table>
             <table schema="" tableName="items"></table>
             <table schema="" tableName="orderdetail"></table>
      -->
               <!-- 有些表的字段需要指定java类型
                <table schema="" tableName="">
                   <columnOverride column="" javaType="" />
               </table> -->
    </context>
</generatorConfiguration>
